{
  "name": "edrea-tailwind-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compileCss": "tailwindcss -i ./public/src/main.css -o ./public/assets/css/main.css --watch",
    "dev": "concurrently \"next dev\" \"npm run compileCss\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export "
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "18.2.0",
    "swiper": "^8.4.5",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4"
  }
}
